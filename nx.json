{
  "npmScope": "spine-z",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "example-app": {
      "tags": ["target:angular"]
    },
    "api": {
      "tags": ["target:backend"]
    },
    "api-interfaces": {
      "tags": ["target:workspace", "type:util"]
    },
    "a-react-lib": {
      "tags": ["target:react"]
    },
    "a-angular-lib": {
      "tags": ["target:angular"]
    },
    "a-angular-feature-lib": {
      "tags": ["target:angular", "type:feature"]
    },
    "a-angular-util-lib": {
      "tags": ["target:angular", "type:util"]
    }
  }
}
